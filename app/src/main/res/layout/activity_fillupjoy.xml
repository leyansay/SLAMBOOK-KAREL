        android:inputType="text"
        android:textColorHint="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/editTextText18"
        app:layout_constraintEnd_toEndOf="@+id/imageView18"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextText18"
        android:layout_width="369dp"
        android:layout_height="61dp"
        android:layout_marginBottom="16dp"
        android:ems="10"
        android:inputType="text"
        android:text="Movie"
        android:textColorHint="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/editTextText19"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextText19"
        android:layout_width="369dp"
        android:layout_height="61dp"
        android:layout_marginBottom="16dp"
        android:ems="10"
        android:inputType="text"
        android:text="Color"
        android:textColorHint="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/editTextText20"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextText20"
        android:layout_width="369dp"
        android:layout_height="61dp"
        android:layout_marginBottom="16dp"
        android:ems="10"
        android:inputType="text"
        android:text="Name"
        android:textColorHint="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/editTextText21"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextText21"
        android:layout_width="369dp"
        android:layout_height="61dp"
        android:layout_marginBottom="40dp"
        android:ems="10"
        android:hint="Animal"
        android:inputType="text"
        android:textColorHint="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/button3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button3"
        android:layout_width="178dp"
        android:layout_height="46dp"
        android:layout_marginBottom="44dp"
        android:backgroundTint="@color/bluejoy"
        android:text="Save"
        app:layout_constraintBottom_toBottomOf="@+id/imageView18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="23dp"
        android:layout_height="34dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="357dp"
        android:layout_marginBottom="826dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/backjoy" />
</androidx.constraintlayout.widget.ConstraintLayout>